{
  "expo": {
    "name": "Exizt",
    "slug": "Exizt",
    "version": "1.0.0",
    "scheme": "myExizt",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#041421",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font"
      ]
    ]
  }
}
